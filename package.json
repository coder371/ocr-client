{
  "name": "ocr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": " react-native run-android --verbose",
    "r-android": " react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "iosSE": "react-native run-ios --simulator='iphone SE0`'",
    "ios8": "react-native run-ios --simulator='ios8'",
    "ios12pm": "react-native run-ios --simulator='ios12pm'",
    "ios14": "react-native run-ios --simulator='iPhone 14'",
    "ios13": "react-native run-ios --simulator='iPhone 13'",
    "ios13p": "react-native run-ios --simulator='iPhone 13 Pro'",
    "ios13pm": "react-native run-ios --simulator='ios13pm'",
    "ios12mini": "react-native run-ios --simulator='ios12mini'",
    "ios8p": "react-native run-ios --simulator='iPhone8p'",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "uninstall": "adb uninstall com.m3akPharm",
    "asset": "react-native-asset",
    "bun": "cd android &&  ./gradlew bundleRelease && open ./app/build/outputs",
    "clean": "cd android && ./gradlew clean && cd .."
  },
