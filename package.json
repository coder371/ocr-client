    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-onboard": "^2.6.0",
    "react-native-onboarding-swiper": "^1.2.0",
    "react-native-paper": "^5.11.4",
    "react-native-safe-area-context": "^4.8.1",
